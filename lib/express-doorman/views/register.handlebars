<h2 class="page-header">Register</h2>

{{#each options.errors}}
    <div class="alert alert-danger">{{message}}</div>
{{/each}}

<form method="post">

    {{#each options.formModel.fields}}
        <div class="form-group">
            <label>{{label}}</label>
            <input class="form-control" placeholder="{{placeholder}}" name="{{name}}" value="{{value}}" type="text">
        </div>
    {{/each}}

    <button type="submit" class="btn btn-default">Submit</button>
</form>